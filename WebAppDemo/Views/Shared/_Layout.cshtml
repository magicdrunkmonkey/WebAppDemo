@{
}

<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>@ViewBag.Title</title>
    </head>
    <body>
        <header>
            <!--Page Logo-->
            <img src="@Url.Content("/Images/small-flower-icon-6.jpg")">

            <!--Navigation menu-->
            <h2>Navigation menu</h2>
            <div class="navbar-header">
                <ul>
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    <li>@Html.ActionLink("Projects", "Projects", "Home")</li>
                    <li>@Html.ActionLink("Fever Check", "FeverCheck", "Doctor")</li>
                    <li>@Html.ActionLink("Guessing Game", "Index")</li>
                </ul>               
            </div>
        </header>
        <main>
            <!--Kanske är den container man talar om i uppgiften-->
            <div>
                @RenderBody()
            </div>
        </main>
        <footer>
            <!-- Date and my name-->
            <p>  @DateTime.Now.Year - George Sydby  </p>
        </footer>
    </body>
</html>